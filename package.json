{
  "name": "env-rx",
  "version": "1.0.0",
  "description": "Zero-config CLI to keep .env and .env.example in sync. Detects missing environment variables and fixes them interactively.",
  "main": "dist/index.js",
  "type": "commonjs",
  "bin": {
    "env-rx": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xserhio/env-rx.git"
  },
  "bugs": {
    "url": "https://github.com/xserhio/env-rx/issues"
  },
  "homepage": "https://github.com/xserhio/env-rx#readme",
  "keywords": [
    "env",
    "dotenv",
    "env-sync",
    "environment-variables",
    "cli",
    "dx",
    "developer-tools",
    "config",
    "sync",
    "automation",
    "validation"
  ],
  "author": "xserhio",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "build": "node build.js",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "npm run typecheck && npm run build"
  },
  "devDependencies": {
    "@types/node": "^14.18.63",
    "@types/prompts": "^2.4.9",
    "esbuild": "^0.27.3",
    "prettier": "^3.8.1",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "dotenv": "^17.3.1",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2"
  }
}